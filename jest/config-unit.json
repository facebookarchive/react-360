{
  "automock": true,
  "transform": {
    "^.+\\.js$": "jest/preprocessor.js"
  },
  "setupFiles": [
    "jest/setup-local.js"
  ],
  "transformIgnorePatterns": [
    "/node_modules/"
  ],
  "testPathIgnorePatterns": [
    "/docs/",
    "/node_modules/",
    "/scripts/",
    "/website/",
    "/EndToEnd/",
    "/react-360-cli/"
  ],
  "modulePathIgnorePatterns": [
    "<rootDir>/docs/",
    "<rootDir>/Examples/",
    "<rootDir>/node_modules/",
    "<rootDir>/scripts/",
    "<rootDir>/tools/",
    "<rootDir>/website/"
  ],
  "haste": {
    "defaultPlatform": "vr",
    "platforms": [
      "vr"
    ]
  },
  "unmockedModulePathPatterns": [
    "node_modules"
  ]
}
